@model List<Category>

<h1> Category List </h1>
<table class="table table-bordered table-striped">
	<thead>
		<tr>
			<th>
				Category Name
			</th>

			<th>
				Display Order
			</th>

			<th>
				Is Verified
			</th>
		</tr>
		<tbody>
			@foreach (var obj in Model.OrderBy(u=> u.DisplayOrder))
			{
			<tr>
				<td>
					@obj.Name
				</td>
				<td>
						
						@obj.DisplayOrder
				</td>
				<td>
					@obj.IsVerified
				</td>
			</tr>
			}

		</tbody>
	</thead>


</table>